    <script>
        var baseServerUrl = '@Url.Content("~")';
        var imageUploadKit = '@Url.Content("~/ClientBin/ImageUploadKit.xap?1")';
        var imageHubUrl = '@ViewData["ImageHubBaseUrl"]';
        $(function () {
            var location = window.location;
            var menu = Request.QueryString("menu");
            $("#firstLevelMenuContainer li").removeClass("active");
            $("#" + menu).addClass("active");

            $("#momentLargeImageBg, #momentLargeImage").on('click', function () {
                $("#momentLargeImageBg, #momentLargeImage").css("display", "none");
            });
        });

        var Request = {
            QueryString: function (item) {
                var svalue = location.search.match(new RegExp("[\?\&]" + item + "=([^\&]*)(\&?)", "i"));
                return svalue ? svalue[1] : svalue;
            }
        }

        function showLargeImage(imageUrl) {
            $("#momentImageUrl").prop("src", imageUrl);
            $("#momentLargeImageBg, #momentLargeImage").css("display", "block");
        }
    </script>
